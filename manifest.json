{
	"manifest_version": 2,
	"name": "Link popup",
	"version": "1.0.2",
	"description": "a extention for Google Chrome",
	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	"browser_action": {
		"default_icon": "icons/icon19.png",
		"default_popup": "popup.html"
	},
	"permissions": [
    "tabs",
		"activeTab",
		"downloads",
		"storage"
	],
	"content_scripts": [
	  {
	    "matches": ["*://*/*"],
			"css": ["mycss.css"],
	    "js": ["jquery-3.1.1.min.js", "myscript.js"]
	  }
	],
	"background": {
	  "scripts": ["jquery-3.1.1.min.js", "background.js"]
	}
}
